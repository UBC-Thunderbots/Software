<launch>
    <!-- Launch the backend_input node -->
    <node name="backend_input" pkg="thunderbots" type="backend_input" output="screen">
    </node>

    <!-- Launch the ai logic node -->
    <node name="ai_logic" pkg="thunderbots" type="ai_logic" output="screen">
    </node>

    <!-- Launch the backend_output node -->
    <node name="backend_output" pkg="thunderbots" type="backend_output" output="screen">
    </node>

    <!-- Launch the dynamic parameters node -->
    <node name="parameters" pkg="thunderbots" type="parameters" output="screen"/>

    <!-- Launch the rqt_reconfigure gui -->
    <node name="rqt_reconfigure" pkg="rqt_reconfigure" type="rqt_reconfigure" output="screen"/>
</launch>
