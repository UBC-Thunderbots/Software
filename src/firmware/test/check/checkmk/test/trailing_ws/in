/* A complete test example (with trailing whitespace) */

#include <stdio.h>

# suite The Suite   

# tcase The Test Case	

# test  the_test    
    int nc;
    const char msg[] = "\n\n    Hello, world!\n";

    nc = printf("%s", msg);
    fail_unless(nc == (sizeof msg
                                  - 1) /* for terminating NUL. */
    );
