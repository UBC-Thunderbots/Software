{
  "name": "corner-kick",
  "description": "A visualization tool for UBC Thunderbots",
  "main": "public/electron.js",
  "author": {
    "name": "UBC Thunderbots",
    "email": "ubcrobocup@gmail.com"
  },
  "homepage": "./",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "3.6.1",
    "autoprefixer": "7.1.6",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "electron-is-dev": "0.3.0",
    "electron-json-storage": "^4.1.4",
    "electron-store": "^2.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "22.4.2",
    "lodash": "^4.17.11",
    "object-assign": "4.1.1",
    "pixi-viewport": "3.0.0",
    "pixi.js": "4.8.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "16.4.1",
    "react-dev-utils": "^5.0.1",
    "react-dom": "16.4.1",
    "react-panelgroup": "^1.0.7",
    "react-pixi-fiber": "0.4.9",
    "resolve": "1.6.0",
    "roslib": "0.20.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "styled-components": "3.4.6",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "unstated": "2.1.1",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "concurrently \"roslaunch rosbridge_server rosbridge_websocket.launch\" \"BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "react-start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "package": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@types/electron-json-storage": "^4.0.0",
    "@types/electron-store": "^1.3.0",
    "@types/inline-style-prefixer": "^3.0.1",
    "@types/jest": "23.3.2",
    "@types/lodash": "^4.14.116",
    "@types/node": "10.9.4",
    "@types/pixi.js": "^4.8.1",
    "@types/react": "16.4.14",
    "@types/react-dom": "16.0.7",
    "babel-core": "6.26.3",
    "babel-runtime": "6.26.0",
    "concurrently": "4.0.1",
    "electron": "2.0.9",
    "electron-builder": "20.28.4",
    "prop-types": "15.6.2",
    "typescript": "2.9.1",
    "wait-on": "3.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "/home/poplitou/Projects/ubcthunderbots/src/corner-kick/tsconfig.test.json"
      }
    }
  },
  "build": {
    "productName": "CornerKick",
    "artifactName": "${productName}.${ext}",
    "directories": {
      "buildResources": "dist-resources"
    },
    "files": [
      "!**/*.d.ts"
    ],
    "linux": {
      "target": [
        "deb"
      ],
      "category": "Development"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "nsis": {
      "oneClick": "false",
      "artifactName": "${productName}-Setup.${ext}",
      "perMachine": true,
      "deleteAppDataOnUninstall": true
    },
    "deb": {
      "packageCategory": "Development"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
